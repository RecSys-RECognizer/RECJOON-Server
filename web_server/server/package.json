{
  "name": "recjoon_server",
  "version": "1.0.0",
  "description": "dev",
  "main": "src/app.ts",
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc",
    "test": "cross-env NODE_ENV=development nodemon --delay 500ms src/app.ts",
    "dev": "cross-env NODE_ENV=production tsc-watch --onSuccess \"ts-node --files -r tsconfig-paths/register dist/app.js\"",
    "prod": "cross-env NODE_ENV=production pm2 start --name recjoon-server ts-node -- --files -r tsconfig-paths/register dist/app.js",
    "docker-test": "cross-env NODE_ENV=production pm2-runtime start --name recjoon-server ts-node -- --files src/app.ts",
    "docker-dev": "cross-env NODE_ENV=production pm2-runtime start --name recjoon-server pm2.json",
    "docker": "cross-env NODE_ENV=production ts-node src/app.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/validator": "^13.7.1",
    "aws-param-store": "^3.2.0",
    "aws-sdk": "^2.1048.0",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "helmet": "^4.6.0",
    "jest": "^27.4.5",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql": "^2.14.1",
    "nodemon": "^2.0.15",
    "package": "^1.0.1",
    "package.json": "^2.0.1",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^3.12.0",
    "tslint": "^6.1.3",
    "typedi": "^0.10.0",
    "typeorm": "0.2.41",
    "typescript": "^4.7.2",
    "validator": "^13.7.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@types/aws-param-store": "^2.1.2",
    "@types/node": "^17.0.35"
  }
}
